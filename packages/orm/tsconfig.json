{
	"extends": "@doubletie/typescript-config/base.json",
	"compilerOptions": {
		"baseUrl": ".",
		"esModuleInterop": true,
		"lib": ["dom", "dom.iterable", "esnext"],
		"paths": { "~/*": ["./src/*"] },
		"target": "ES2022",
		"skipLibCheck": true,
		"noImplicitAny": false,
		"moduleResolution": "node"
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
