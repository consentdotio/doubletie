{
	"name": "@doubletie/orm",
	"version": "0.0.1",
	"description": "Type safe ORM for kysely",
	"repository": {
		"type": "git",
		"url": "git://github.com/seeden/kysely-orm.git"
	},
	"engines": {
		"node": ">=16.2.0"
	},
	"type": "module",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"scripts": {
		"clean": "rm -rf dist",
		"fmt": "pnpm biome format --write . && biome check --formatter-enabled=false --linter-enabled=false --organize-imports-enabled=true --write",
		"lint": "pnpm biome lint ./src",
		"build": "rslib build",
		"test": "vitest run",
		"test:watch": "vitest",
		"prepublishOnly": "npm run build",
		"valid": "tsc --noEmit"
	},
	"author": "Christopher Burns <christopher@doubletie.com>",
	"license": "MIT",
	"dependencies": {
		"base-x": "^5.0.1",
		"url-slug": "^4.0.1"
	},
	"devDependencies": {
		"@doubletie/typescript-config": "workspace:*",
		"@types/better-sqlite3": "^7.6.12",
		"@types/node": "^22.13.10",
		"@types/pg": "^8.11.11",
		"better-sqlite3": "^11.8.1",
		"dotenv": "^16.4.7",
		"kysely": "0.27.6",
		"typescript": "5.8.2",
		"vitest": "^3.0.8"
	},
	"peerDependencies": {
		"kysely": "~0.27.6"
	}
}
